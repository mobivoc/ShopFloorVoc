# Reliability Metrics Properties

scor:hasPerfectOrderFulfillment a owl:DatatypeProperty;
	rdfs:label "has Perfect Order Fulfillment" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double ;
	scor:hasMetricID "RL1.1" ;
	scor:isCalculatedBy "[Total Perfect Orders] / [Total Number of Orders] x 100%" .
		
scor:hasPercentOfOrdersDeliveredInFull a owl:DatatypeProperty;
	rdfs:label "has Percent of Orders Delivered In Full" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double ;
	scor:hasMetricID "RL2.1" ;
	scor:isCalculatedBy "[Total number of orders delivered in full] / [Total number of orders delivered] x 100%" .

scor:hasDeliveryPerformanceToCustomerCommitDate a owl:DatatypeProperty;
	rdfs:label "has Percent of Orders Delivered In Full" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double ;
	scor:hasMetricID "RL2.2" ;
	scor:isCalculatedBy "[Total number of orders delivered on the original commitment date] / [Total number of orders delivered] x 100%" .

scor:hasDocumentationAccuracy a owl:DatatypeProperty;
	rdfs:label "Documentation Accuracy" ;
	rdfs:comment "Percentage of orders with on time and accurate documentation supporting the order, including
packing slips, bills of lading, invoices, etc." ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double ;
	scor:hasMetricID "RL2.3" ;
	scor:isCalculatedBy "[Total number of orders delivered with accurate documentation] / [Total number of orders delivered]
x 100%" .

scor:hasPerfectCondition a owl:DatatypeProperty;
	rdfs:label "Perfect Condition" ;
	rdfs:comment "Percentage of orders delivered in an undamaged state that meet specification, have the correct
configuration, are faultlessly installed (as applicable) and accepted by the customer." ;
	scor:hasMetricID "RL2.4" ;
	scor:isCalculatedBy "[ Number of orders delivered in Perfect Condition ] / [Number of orders delivered ] x 100%" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasDeliveryItemAccuracy a owl:DatatypeProperty;
	scor:hasMetricID "RL3.33" ;
	rdfs:label "Delivery Item Accuracy" ;
	scor:isCalculatedBy "Percentage of orders in which all items ordered are
the items actually provided, and no extra items are provided" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasDeliveryQuantityAccuracy a owl:DatatypeProperty;
	rdfs:label "Delivery Quantity Accuracy" ;
	scor:hasMetricID "RL3.35" ;
	scor:isCalculatedBy "Percentage of orders which is delivered to the correct location and customer entity" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasCustomerCommitDateAchievementTimeCustomerReceiving a owl:DatatypeProperty;	
	rdfs:label "Delivery Performance to Customer Commit Date" ;
	scor:hasMetricID "RL3.32" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasDeliveryLocationAccuracy a owl:DatatypeProperty;
	rdfs:label "Delivery Location Accuracy" ;
	scor:hasMetricID "RL3.34" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasComplianceDocumentationAccuracy a owl:DatatypeProperty;
	rdfs:label "Compliance Documentation Accuracy" ;
	scor:hasMetricID "RL3.31" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasOtherRequiredDocumentationAccuracy a owl:DatatypeProperty;	
	rdfs:label "Other Required Documentation Accuracy" ;
	scor:hasMetricID "RL3.43" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasPaymentDocumentationAccuracy a owl:DatatypeProperty;
	rdfs:label "Payment Documentation Accuracy" ;
	scor:hasMetricID "RL3.45" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasShippingDocumentationAccuracy a owl:DatatypeProperty;	
	rdfs:label "Shipping Documentation Accuracy" ;
	scor:hasMetricID "RL3.50" ;
	scor:isCalculatedBy "" ;
	rdfs:comment "" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:double .

scor:hasPercentOfFaultlessInstallations a owl:DatatypeProperty;	
	rdfs:label "% Of Faultless Installations" ;
	scor:hasMetricID "RL3.12" ;
	rdfs:comment "" .

scor:hasPercentOfOrdersOrLinesReceivedDamageFree a owl:DatatypeProperty;		
	rdfs:label "% Orders/lines received damage free" ;
	scor:hasMetricID "RL3.24" ;
	rdfs:comment "" .

scor:hasOrdersDeliveredDamageFreeConformance a owl:DatatypeProperty;		
	rdfs:label "Orders Delivered Damage Free Conformance" ;
	scor:hasMetricID "RL3.41" ;
	rdfs:comment "" .

scor:hasOrdersDeliveredDefectFreeConformance a owl:DatatypeProperty;	
	rdfs:label "Orders Delivered Defect Free Conformance" ;
	scor:hasMetricID "RL3.42" ;
	rdfs:comment "" .

scor:hasWarrantyAndReturns a owl:DatatypeProperty;		
	rdfs:label "Warranty and Returns" ;
	scor:hasMetricID "RL3.55" ;
	rdfs:comment "" .


# Properties

scor:hasMetricID a owl:AnnotationProperty ;
	rdfs:label "has metric ID" ;
	rdfs:comment "" ;
	rdfs:range xsd:string .

scor:isCalculatedBy a owl:AnnotationProperty ;
	rdfs:label "is calculated by" ;
	rdfs:range xsd:string .

scor:hasProcessID a owl:AnnotationProperty ;
	rdfs:label "has process ID" ;
	rdfs:comment "represents a unique identifier of a process within the SCOR model" ;
	rdfs:range xsd:string .

scor:hasProcessStepList a owl:ObjectProperty;
	rdfs:label "has process step list";
	rdfs:comment "represents a unique identifier of a process within the SCOR model" ;
	rdfs:domain scor:Process ;
	rdfs:range xsd:string .



