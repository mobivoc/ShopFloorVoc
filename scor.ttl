scor:People a owl:Class ;
	rdfs:label "People" ;
	rdfs:comment "". 

scor:Practices a owl:Class ;
	rdfs:label "Practices" ;
	rdfs:comment "". 

# scor Makro Processes

scor:Plan a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Plan";
	rdfs:comment "The Plan processes describe the planning activities associated with operating a supply chain. This
includes gathering customer requirements, collecting information on available resources, and balancing
requirements and resources to determine planned capabilities and resource gaps. This is followed by
identifying the actions required to correct any gaps." ;
	scor:hasProcessID "P" .

scor:Source a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Source" ;
	scor:hasProcessID "S" .

scor:Make a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Make" ;
	scor:hasProcessID "M" .

scor:Deliver a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Deliver" ;
	scor:hasProcessID "D" .

scor:Return a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Return" ;
	scor:hasProcessID "R" .

scor:Enable a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Enable" ;
	scor:hasProcessID "E" .

